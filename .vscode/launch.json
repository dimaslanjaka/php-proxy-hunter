{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "command": "python main.py",
      "name": "Run app",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python main_proxy_manager.py",
      "name": "Run proxy manager",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python dl_traffic.py",
      "name": "Run selenium",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "command": "php proxyChecker.php && php proxySocksChecker.php",
      "name": "Check proxies",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Debug index.js",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd",
      "args": [
        "."
      ],
      "outputCapture": "std"
    },
    {
      "command": "composer exec php-cs-fixer fix",
      "name": "php-cs-fixer",
      "request": "launch",
      "type": "node-terminal"
    },
  ]
}