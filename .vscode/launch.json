{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "python main.py",
      "name": "Run app",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python main_proxy_manager.py",
      "name": "Run proxy manager",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python profile_manager.py",
      "name": "Run profile manager",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python proxyCheckerMultiThreading.main.py",
      "name": "Run proxy checker",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python device_info.main.py",
      "name": "Run device info",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python runner.main.py",
      "name": "Run selenium",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python workingManager.py",
      "name": "Run working.json editor",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "command": "python manage.py test django_backend.apps.proxy",
      "name": "Django test proxy",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Debug index.js",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd",
      "args": [
        "."
      ],
      "outputCapture": "std"
    },
    {
      "command": "composer exec php-cs-fixer fix",
      "name": "php-cs-fixer",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python manage.py runserver 0.0.0.0:8000",
      "name": "Run django",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python manage.py runserver_plus 0.0.0.0:8000 --cert-file django_backend/certificates/localhost.crt --key-file django_backend/certificates/localhost.key",
      "name": "Run django SSL",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "python manage.py run_huey",
      "name": "Run huey",
      "request": "launch",
      "type": "node-terminal"
    }
  ],
  "compounds": [
    {
      "name": "Run Django with Huey",
      "configurations": [
        "Run django SSL",
        "Run huey"
      ]
    }
  ]
}