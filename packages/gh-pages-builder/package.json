{
  "name": "gh-pages-builder",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Pages documentation builder for markdown files",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./config": {
      "import": "./dist/esm/config.js",
      "require": "./dist/cjs/config.js"
    }
  },
  "bin": {
    "gh-build": "./bin/build-gh-pages.js",
    "gh-pages-builder": "./bin/build-gh-pages.js"
  },
  "type": "module",
  "license": "ISC",
  "author": {
    "name": "Dimas Lanjaka",
    "email": "dimaslanjaka@gmail.com",
    "url": "https://webmanajemen.com"
  },
  "repository": {
    "url": "https://github.com/dimaslanjaka/php-proxy-hunter",
    "type": "git",
    "directory": "packages/gh-pages-builder"
  },
  "keywords": [
    "github-pages",
    "documentation",
    "markdown",
    "toc",
    "builder"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --watch",
    "dev": "node src/build-gh-pages.js --dev",
    "clean": "rimraf dist",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "cross-env-shell NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "nunjucks": "^3.2.4",
    "sbg-utility": "^2.0.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/markdown-it": "^14",
    "@types/node": "^24.0.7",
    "@types/nunjucks": "^3.2.6",
    "binary-collections": "^2.0.6",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
