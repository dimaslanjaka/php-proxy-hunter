#!/bin/bash

if [ -f .env ]; then
    # Load the .env file
    export $(grep -v '^#' .env | xargs)
    echo ".env file loaded"
fi

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
echo "Current script directory: $SCRIPT_DIR"
CWD="$(dirname "$SCRIPT_DIR")"
node "$SCRIPT_DIR/create-file-hashes.js"